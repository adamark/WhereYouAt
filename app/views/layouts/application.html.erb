<!DOCTYPE html>
<html>
<head>
  <title>WhereYouAt</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |key, value| %>
    <div class="flash <%= key %>"><%= value %></div>
  <% end %>
<div id="main">
<%= yield %>
</div>
</body>
</html>
